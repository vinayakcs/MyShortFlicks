var utils=msf_utils();
<% for channel in @channels %>
$('#channel_preview_container').append("<%=  escape_javascript render :partial => 'channels/preview_channel', :locals => {:channel => channel} %>");
<%end%>
<% if @isLastResults==true %>
$('#channel_preview_container').after("<%= escape_javascript render :partial => 'shared/noresult' %>");
$('#load_more_link').remove();
<%else%> 
var oldUrl=$('#load_more_link').attr('href');
$('#load_more_link').attr('href',utils.setLoadMoreParam(oldUrl));
<%end%>
